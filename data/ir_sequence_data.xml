<?xml version="1.0" encoding="utf-8"?>
<odoo noupdate="1">

    <record id="customer_id" model="ir.sequence">
        <field name="name">Customer Id</field>
        <field name="code">customer.unique.id</field>
        <field name="prefix">Id</field>
        <field name="padding">3</field>
        <field name="company_id" eval="False"/>
    </record>
    <record id="customer_invoices_id" model="ir.sequence">
        <field name="name">Customer Invoice Id</field>
        <field name="code">customer.invoice.id</field>
        <field name="prefix">INV/%(year)s/</field>
        <field name="padding">4</field>
        <field name="company_id" eval="False"/>
    </record>

</odoo>